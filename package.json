{
  "name": "test-crawlee",
  "version": "0.0.1",
  "description": "This is an example of a Crawlee project.",
  "type": "module",
  "dependencies": {
    "@crawlee/types": "^3.0.0",
    "@google-cloud/bigquery": "^6.0.2",
    "@google-cloud/pubsub": "^3.1.0",
    "@google-cloud/tasks": "^3.0.0",
    "body-parser": "^1.20.0",
    "crawlee": "^3.1.2",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-fetch": "^3.2.10",
    "pino": "^8.4.2",
    "playwright": "^1.25.0",
    "playwright-core": "*",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@apify/tsconfig": "^0.1.0",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.188",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "jest": "^28.0.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "start": "npm run start:dev",
    "start:prod": "node --experimental-specifier-resolution=node dist/index.js",
    "start:dev": "ts-node-esm --experimental-specifier-resolution=node -T src/index.ts",
    "run-main": "rm -r storage && NODE_OPTIONS='--max-old-space-size=4096' ts-node-esm --experimental-specifier-resolution=node -T src/main.ts",
    "build": "tsc",
    "test": "jest",
    "demo": "tsc && node demo.js",
    "docker": "npm run build && docker build -t test-crawlee . && docker run --rm -p 8080:8080 -e PORT=8080 test-crawlee"
  },
  "author": "It's not you it's me",
  "license": "ISC"
}
